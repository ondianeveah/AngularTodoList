<?xml version="1.0" encoding="utf-8"?>
<DeviceMetricsConfig>
	<SettingList>
		<Setting Key="ReportUsageStatistics">true</Setting>
		<Setting Key="ConfigFileUpdateFrequency">7</Setting>
		<Setting Key="MinimumRunDays">1</Setting>
		<Setting Key="DebugLevel">0</Setting>
		<Setting Key="ErrorLoggingEnabled">true</Setting>
	</SettingList>
	
	<CustomSettingList>
		<CustomSetting name="ReportOpticalDriveMetrics" sendFrequency="7">
            <CustomItemList>
                <CustomItem name="OpticalDriveInfo">true</CustomItem>
            </CustomItemList>
		</CustomSetting>

		<CustomSetting name="ReportDriveStorageMetrics" sendFrequency="0">
            <CustomItemList>
                <CustomItem name="DriveStorage">true</CustomItem>
            </CustomItemList>
		</CustomSetting>

		<CustomSetting name="ReportCardReaderMetrics" sendFrequency="0">
            <CustomItemList>
                <CustomItem name="CardReaderInfo">true</CustomItem>
            </CustomItemList>
		</CustomSetting>
		
		<CustomSetting name="ReportSystemUpdateMetrics" sendFrequency="7">
            <CustomItemList>
                <CustomItem name="SystemUpdate">true</CustomItem>
            </CustomItemList>
            <CustomParamList>
                <CustomParam name="LenovoUpdates">true</CustomParam>
                <CustomParam name="WindowsUpdates">true</CustomParam>
			</CustomParamList>
		</CustomSetting>
		
		<CustomSetting name="ReportNeverCrashMetrics" sendFrequency="7">
            <CustomItemList>
                <CustomItem name="NeverCrash">true</CustomItem>
            </CustomItemList>
            <CustomParamList>
				<CustomParam name="MTBF">false</CustomParam>
				<CustomParam name="WERBlob">true</CustomParam>
			</CustomParamList>
		</CustomSetting>

		<CustomSetting name="ReportPowerMetrics" sendFrequency="0">
            <CustomItemList>
                <CustomItem name="ACDCInfo">true</CustomItem>
            </CustomItemList>
		</CustomSetting>

		<CustomSetting name="ReportSystemInfoMetrics" sendFrequency="0">
            <CustomItemList>
                <CustomItem name="SystemInfo">true</CustomItem>
            </CustomItemList>
            <CustomParamList>
                <CustomParam name="SysInfoBlob">true</CustomParam>
				<CustomParam name="StartupPrograms">true</CustomParam>
			</CustomParamList>
		</CustomSetting>
		
		<CustomSetting name="ReportBatteryMetrics" sendFrequency="7">
            <CustomItemList>
                <CustomItem name="Battery">true</CustomItem>
            </CustomItemList>
            <CustomParamList>
                <CustomParam name="RawData">true</CustomParam>
                <CustomParam name="ProcessedData">true</CustomParam>
            </CustomParamList>
		</CustomSetting>
		
		<CustomSetting name="ReportInstalledDrivers" sendFrequency="7">
			<CustomItemList>
                <CustomItem name="SystemDrivers">false</CustomItem>
                <CustomItem name="DeviceDrivers">false</CustomItem>
			</CustomItemList>
		</CustomSetting>
		
		<CustomSetting name="ReportDriveStorage" sendFrequency="0">
			<CustomItemList>
                <CustomItem name="LocalDrives">true</CustomItem>
			</CustomItemList>
		</CustomSetting>
		
		<CustomSetting name="ReportSoftwareUsage" sendFrequency="0">
			<CustomItemList>
                <CustomItem name="SoftwareUsage">false</CustomItem>
			</CustomItemList>
            <CustomParamList>
                <CustomParam name="UsageTime">true</CustomParam>
				<CustomParam name="BlacklistApps">adctl.exe, lplatsvc.exe, fmapp.exe</CustomParam>
            </CustomParamList>
		</CustomSetting>

		<CustomSetting name="ReportUSBMetrics" CXPsendFrequency="1 week">
            <CustomItemList>
                <CustomItem name="USBInfo">true</CustomItem>
            </CustomItemList>
		</CustomSetting>

		<CustomSetting name="ReportUserInfoMetrics" CXPsendFrequency="1 day">
			<CustomItemList>
				<CustomItem name="UserInfo">true</CustomItem>
			</CustomItemList>
		</CustomSetting>

		<CustomSetting name="ReportWindowsSessionMetrics" CXPsendFrequency="1 week">
            <CustomItemList>
                <CustomItem name="WindowsSessionInfo">true</CustomItem>
            </CustomItemList>
            <CustomParamList>
                <CustomParam name="SessionActivityBlob">true</CustomParam>
			</CustomParamList>
		</CustomSetting>

		<CustomSetting name="ReportWifiMetrics" CXPsendFrequency="1 week" CXPcollectFrequency="1 hour">
			<CustomItemList>
				<CustomItem name="WifiInfo">true</CustomItem>
			</CustomItemList>			
		</CustomSetting>

		<CustomSetting name="ReportMouseActivityMetrics" CXPsendFrequency="1 day" CXPcollectFrequency="10 minutes">
			<CustomItemList>
				<CustomItem name="MouseActivity">true</CustomItem>
			</CustomItemList>			
		</CustomSetting>

		<CustomSetting name="ReportModeActivityMetrics" CXPsendFrequency="1 week">
			<CustomItemList>
				<CustomItem name="ModeActivity">true</CustomItem>
			</CustomItemList>			
		</CustomSetting>

		<CustomSetting name="ReportKeyboardActivityMetrics" CXPsendFrequency="1 day">
			<CustomItemList>
				<CustomItem name="KeyboardActivity">true</CustomItem>
			</CustomItemList>			
		</CustomSetting>

		<CustomSetting name="ReportCameraActivityMetrics" CXPsendFrequency="1 day">
			<CustomItemList>
				<CustomItem name="CameraActivity">true</CustomItem>
			</CustomItemList>			
		</CustomSetting>

		<CustomSetting name="ReportPortMetrics" CXPsendFrequency="1 day">
			<CustomItemList>
				<CustomItem name="PortInfo">true</CustomItem>
			</CustomItemList>			
		</CustomSetting>

		<CustomSetting name="ReportForegroundAppUseMetrics" CXPsendFrequency="1 day">
            <CustomItemList>
                <CustomItem name="ForegroundUse">true</CustomItem>
            </CustomItemList>
			<CustomParamList>
				<CustomParam name="CacheSize">100</CustomParam>
				<CustomParam name="CacheWriteTime">10</CustomParam>
			</CustomParamList>
		</CustomSetting>

		<CustomSetting name="ReportDisplayMetrics" CXPsendFrequency="1 day">
            <CustomItemList>
                <CustomItem name="Display">true</CustomItem>
            </CustomItemList>
            <CustomParamList>
				<CustomParam name="DisplayPluginData">false</CustomParam>
				<CustomParam name="DisplayActivityData">true</CustomParam>
			</CustomParamList>
		</CustomSetting>

	</CustomSettingList>
